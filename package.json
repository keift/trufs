{
  "name": "trufs",
  "version": "1.2.0",
  "description": "Trusty file methods.",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev:symbolic": "bun run --watch ./examples/symbolic.ts",
    "test": "npm run test:snowflake && npm run test:symbolic",
