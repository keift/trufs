    "build": "tsup ./src/index.ts --dts --clean --format esm,cjs --out-dir ./dist"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/graceful-fs": "^4.1.9",
    "atomically": "^2.0.3",
    "fs-extra": "^11.3.0",
    "graceful-fs": "^4.2.11"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keift/trufs.git"
  },
  "keywords": [
    "fs",
    "file-system",
    "graceful-fs",
    "fs-extra",
    "atomic",
    "atomically",
    "atomic-reads",
    "atomic-writes"
  ]
